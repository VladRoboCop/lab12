@model List<Company>

<h1>Список компаний</h1>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Название</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var company in Model)
        {
            <tr>
                <td>@company.Id</td>
                <td>@company.Name</td>
            </tr>
        }
    </tbody>
</table>
